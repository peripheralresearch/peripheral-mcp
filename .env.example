# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_key_here

# MCP Auth Tokens (comma-separated bearer tokens for cloud access)
# Empty = open access (no auth). Generate tokens: python -c "import secrets; print(secrets.token_urlsafe(32))"
MCP_AUTH_TOKENS=

# Peripheral API URL (for local MCP server to call FastAPI)
# Only needed for src.mcp.server (not cloud_server)
PERIPHERAL_API_URL=http://localhost:8000

# Prefect Cloud (optional - for workflow automation)
PREFECT_API_URL=
PREFECT_API_KEY=

# OpenAI (optional - for future semantic search)
OPENAI_API_KEY=
